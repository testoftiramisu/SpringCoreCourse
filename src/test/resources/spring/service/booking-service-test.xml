<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <import resource="../dao/ticket-dao-test.xml"/>
    <import resource="../domain/auditoriums.xml"/>

    <bean id="eventService" class="com.epam.spring.hometask.service.booking.CinemaBookingService">
        <constructor-arg ref="ticketDao"/>
        <property name="highRateCoefficient" value="${highRateCoefficient}"/>
        <property name="vipSeatsCoefficient" value="${vipSeatsCoefficient}"/>
    </bean>

</beans>

