<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <import resource="../service/id-generator-service.xml"/>
    <import resource="../service/event-service-test.xml"/>
    <import resource="../domain/users.xml"/>
    <import resource="../domain/tickets.xml"/>

    <bean id="ticketDao" class="com.epam.spring.hometask.dao.ticket.CinemaTicketDao">
        <constructor-arg ref="idGeneratorService"/>
        <property name="tickets" ref="testTickedOne"/>
    </bean>

</beans>

